{
  "name": "@rocketc/shortcuts",
  "version": "0.0.1",
  "description": "pure javascript shortcut solution",
  "keywords": [
    "javascript",
    "shortcut"
  ],
  "license": "MIT",
  "author": "chenfq95 <chenfq95@foxmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenfq95/rocketc.git",
    "directory": "packages/libs/shortcuts"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
