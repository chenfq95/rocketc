    "test": "vitest"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.55.2",
    "@types/node": "^24.10.4",
    "happy-dom": "^20.0.11",
    "oxfmt": "^0.23.0",
    "oxlint": "^1.36.0",
    "typescript": "^5.9.3",
    "unplugin-dts": "1.0.0-beta.6",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "lint-staged": {
    "**/*.(json|css|scss|md)": [
      "pnpm format"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "pnpm lint:fix"
    ]
  }
